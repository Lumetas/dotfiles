{
	"distros": "@include:distros.json",
	"currentDistro": "fedora",
	"extraPkgs": {
		"dwm/dwm": "git@github.com:Lumetas/dwm.git",
		"nvim/.config/nvim": "git@github.com:Lumetas/nvim-cfg.git"
	},
	"WM": {
		"dwm": {
			"active": true,
			"build": "cd ~/dwm && sudo make clean install"
		},
		"hyprland": {
			"active": false
		}
	},
	"PKGS": {
		"fish": {
			"active": true
		},
		"nvim": {
			"active": true
		},
		"kitty": {
			"active": true
		},
		"wallpapers": {
			"active": true
		}
	},
	"dependencies": "@include:deps.json",
	"services": {
		"ssh-server": {
			"fedora": {
				"start": true,
				"enable": true,
				"name": "sshd"
			}
		}
	},
	"system": {
		"hostname": "lumetas",
		"timezone": "Asia/Yekaterinburg",
		"locale": "ru_UA.UTF-8",
		"keymap": "us"
	},
	"users": [
		{
			"name": "lum",
			"home": "/home/lum",
			"shell": "/bin/fish",
			"groups": [
				"lum",
				"wheel",
				"video",
				"input",
				"docker"
			],
			"enabled": true,
			"lock": false
		}
	]
}

